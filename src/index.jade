    link(href="main.min.css", rel="stylesheet")
    
    script.
      WebFontConfig = {
        active: function() {
          sessionStorage.fonts = true;
        },
        google: {
          families: ["Raleway:100,300,600"]
        }
      };
      (function() {
        if(sessionStorage.fonts) {
          console.info("Fonts installed");
          document.documentElement.classList.add("wf-active");
        } else {
          console.info("No fonts installed");
        }
      })();

    script(async, defer, src="https://ajax.googleapis.com/ajax/libs/webfont/1.5.18/webfont.js")
